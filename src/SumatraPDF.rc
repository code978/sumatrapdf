    DEFPUSHBUTTON   "Yes",IDOK,6,40,32,14
    PUSHBUTTON      "No",IDCANCEL,160,40,32,14
END

IDD_DIALOG_INVERSESEARCH DIALOGEX 0, 0, 221, 64
