# cf. https://sumatrapdf.googlecode.com/svn/trunk/docs/SumatraPDF-user.sqt
AdvancedPrefs [
	TraditionalEbookUI = true
	ReuseInstance = false
	MainWindowBackground = #00ff00
	EscToExit = false
	TextColor = #0000ff
	PageColor = #ff0000
	ZoomIncrement = 0
	ZoomLevels = 8.33 12.5 18 25 33.33 50 66.67 75 100 125 150 200 300 400 600 800 1000 1200 1600 2000 2400 3200 4800 6400
]
PrinterDefaults [
	PrintScale = shrink
	PrintAsImage = false
]
PagePadding [
	OuterX = 2
	OuterY = 2
	InnerX = 4
	InnerY = 4
]
BackgroundGradient [
	Enabled = false
	ColorTop = #2828aa
	ColorMiddle = #28aa28
	ColorBottom = #aa2828
]
ForwardSearch [
	HighlightOffset = 0
	HighlightWidth = 0
	HighlightColor = #000000
	HighlightPermanent = false
]
ExternalViewer [
	CommandLine = viewer1.exe
] [
	CommandLine = viewer2.exe
] [
	CommandLine = viewer3.exe
]
